{
	"name": "xcad",
	"type": "dark",
	"colors": {
		"editor.background": "#141414",
		"editor.foreground": "#F1F1F1",
		"editorCursor.foreground": "#FFFFFF",
		"editor.selectionBackground": "#2A2A2A",
		"editor.lineHighlightBackground": "#1E1E1E",
		"editorLineNumber.foreground": "#888888",
		"editorLineNumber.activeForeground": "#FFFFFF",
		"editorWhitespace.foreground": "#444444",
		"editorIndentGuide.background": "#1A1A1A",
		"editorIndentGuide.activeBackground": "#5C78FF",
		"activityBar.background": "#121212",
		"activityBar.foreground": "#F1F1F1",
		"sideBar.background": "#1A1A1A",
		"sideBar.foreground": "#F1F1F1",
		"statusBar.background": "#121212",
		"statusBar.foreground": "#F1F1F1",
		"titleBar.activeBackground": "#121212",
		"titleBar.activeForeground": "#F1F1F1",
		"tab.activeBackground": "#141414",
		"tab.inactiveBackground": "#121212",
		"tab.activeForeground": "#F1F1F1",
		"tab.inactiveForeground": "#888888",
		"terminal.background": "#1A1A1A",
		"terminal.foreground": "#F1F1F1",
		"terminal.ansiBlack": "#121212",
		"terminal.ansiRed": "#A52AFF",
		"terminal.ansiGreen": "#7129FF",
		"terminal.ansiYellow": "#3D2AFF",
		"terminal.ansiBlue": "#2B4FFF",
		"terminal.ansiMagenta": "#2883FF",
		"terminal.ansiCyan": "#28B9FF",
		"terminal.ansiWhite": "#F1F1F1",
		"terminal.ansiBrightBlack": "#666666",
		"terminal.ansiBrightRed": "#BA5AFF",
		"terminal.ansiBrightGreen": "#905AFF",
		"terminal.ansiBrightYellow": "#685AFF",
		"terminal.ansiBrightBlue": "#5C78FF",
		"terminal.ansiBrightMagenta": "#5EA2FF",
		"terminal.ansiBrightCyan": "#5AC8FF",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"gitDecoration.modifiedResourceForeground": "#FFD066",
		"gitDecoration.deletedResourceForeground": "#BA5AFF",
		"gitDecoration.untrackedResourceForeground": "#32CBFF",
		"gitDecoration.stageModifiedResourceForeground": "#6EB8FF",
		"gitDecoration.stageDeletedResourceForeground": "#BA5AFF",
		"gitDecoration.ignoredResourceForeground": "#666666",
		"debugToolBar.background": "#121212",
		"debugToolBar.border": "#2A2A2A"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#777777",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C173FF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#A070FF"
			}
		},
		{
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#F1F1F1"
			}
		},
		{
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#B38F00"
			}
		},
		{
			"scope": [
				"string",
				"constant.other.symbol",
				"punctuation.definition.string",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#32CB32"
			}
		},
		{
			"scope": [
				"function",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#6EB8FF"
			}
		},
		{
			"scope": [
				"type",
				"entity.name.type",
				"storage.type",
				"support.class"
			],
			"settings": {
				"foreground": "#3299FF"
			}
		},
		{
			"scope": [
				"punctuation",
				"operator",
				"keyword.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.logical",
				"keyword.operator.comparison"
			],
			"settings": {
				"foreground": "#32B9FF"
			}
		},
		{
			"scope": [
				"number",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#FFD066",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"boolean",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#BA5AFF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"variable.language.this",
				"variable.other.property",
				"entity.other.attribute-name",
				"variable.other.member"
			],
			"settings": {
				"foreground": "#FF2A9D"
			}
		}
	],
	"semanticTokenColors": {
		"enabled": true,
		"rules": {
			"variable.property": "#FF2A9D", // propriet√† membri classe / this.proprieta
			"variable.other.property": "#FF2A9D", // fallback per JS/TS
			"variable.parameter": "#B38F00", // parametri
			"variable": "#F1F1F1", // variabili generiche
			"this": "#FF2A9D", // 'this' stesso
			"class": "#3299FF", // classi/nomi tipi
			"function": "#6EB8FF", // funzioni/metodi
			"keyword": "#C173FF", // keywords
			"string": "#32CB32", // stringhe e apici/virgole
			"number": "#FFD066", // numeri
			"boolean": "#BA5AFF" // booleani
		}
	}
}