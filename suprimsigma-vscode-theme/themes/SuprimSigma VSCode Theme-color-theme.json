{
  "$schema": "vscode://schemas/color-theme",
  "name": "SuprimSigma Enhanced",
  "type": "dark",
  "colors": {
    // ========== EDITOR PRINCIPALE ==========
    "editor.background": "#1A1625",
    "editor.foreground": "#C5C9D1",
    "editorCursor.foreground": "#A070FF",
    "editor.selectionBackground": "#2D2640",
    "editor.inactiveSelectionBackground": "#252034",
    "editor.selectionHighlightBackground": "#2A243A",
    "editor.lineHighlightBackground": "#1E1A2A",
    "editor.lineHighlightBorder": "#00000000",
    "editor.wordHighlightBackground": "#2D2640",
    "editor.wordHighlightStrongBackground": "#3A3050",
    "editor.findMatchBackground": "#5C78FF40",
    "editor.findMatchHighlightBackground": "#5C78FF20",
    "editor.hoverHighlightBackground": "#2A243A",
    "editor.rangeHighlightBackground": "#2A243A20",
    
    // ========== EDITOR GUTTER & LINE NUMBERS ==========
    "editorLineNumber.foreground": "#6A6482",
    "editorLineNumber.activeForeground": "#A89FC8",
    "editorWhitespace.foreground": "#3A3048",
    "editorIndentGuide.background": "#2A2438",
    "editorIndentGuide.activeBackground": "#5C78FF",
    "editorRuler.foreground": "#2A2438",
    
    // ========== EDITOR BRACKET MATCHING ==========
    "editorBracketMatch.background": "#5C78FF30",
    "editorBracketMatch.border": "#5C78FF",
    "editorBracketHighlight.foreground1": "#6EB8FF",
    "editorBracketHighlight.foreground2": "#C173FF",
    "editorBracketHighlight.foreground3": "#32CB32",
    "editorBracketHighlight.foreground4": "#FFD066",
    "editorBracketHighlight.foreground5": "#FF2A9D",
    "editorBracketHighlight.foreground6": "#32B9FF",
    
    // ========== EDITOR ERRORS & WARNINGS ==========
    "editorError.foreground": "#FF5F87",
    "editorWarning.foreground": "#FFB347",
    "editorInfo.foreground": "#6EB8FF",
    "editorHint.foreground": "#A070FF",
    
    // ========== EDITOR WIDGETS ==========
    "editorWidget.background": "#1C1826",
    "editorWidget.border": "#3A3050",
    "editorSuggestWidget.background": "#1C1826",
    "editorSuggestWidget.border": "#3A3050",
    "editorSuggestWidget.selectedBackground": "#2D2640",
    "editorHoverWidget.background": "#1C1826",
    "editorHoverWidget.border": "#3A3050",
    
    // ========== ACTIVITY BAR ==========
    "activityBar.background": "#13101C",
    "activityBar.foreground": "#B8B4C8",
    "activityBar.inactiveForeground": "#6A6482",
    "activityBar.border": "#1E1A2A",
    "activityBarBadge.background": "#5C78FF",
    "activityBarBadge.foreground": "#FFFFFF",
    
    // ========== SIDEBAR ==========
    "sideBar.background": "#16131F",
    "sideBar.foreground": "#C5C9D1",
    "sideBar.border": "#1E1A2A",
    "sideBarTitle.foreground": "#B8B4C8",
    "sideBarSectionHeader.background": "#1A1625",
    "sideBarSectionHeader.foreground": "#B8B4C8",
    "sideBarSectionHeader.border": "#2A2438",
    
    // ========== LISTS & TREES ==========
    "list.activeSelectionBackground": "#2D2640",
    "list.activeSelectionForeground": "#E0E4EB",
    "list.inactiveSelectionBackground": "#252034",
    "list.inactiveSelectionForeground": "#C5C9D1",
    "list.hoverBackground": "#1E1A2A",
    "list.hoverForeground": "#E0E4EB",
    "list.focusBackground": "#2D2640",
    "list.focusForeground": "#E0E4EB",
    "list.highlightForeground": "#6EB8FF",
    
    // ========== TABS ==========
    "tab.activeBackground": "#1A1625",
    "tab.inactiveBackground": "#13101C",
    "tab.activeForeground": "#E0E4EB",
    "tab.inactiveForeground": "#7A7488",
    "tab.activeBorder": "#5C78FF",
    "tab.activeBorderTop": "#00000000",
    "tab.border": "#1E1A2A",
    "tab.hoverBackground": "#1E1A2A",
    "tab.hoverForeground": "#C5C9D1",
    "tab.unfocusedActiveForeground": "#B8B4C8",
    "tab.unfocusedInactiveForeground": "#6A6482",
    
    // ========== TITLE BAR ==========
    "titleBar.activeBackground": "#13101C",
    "titleBar.activeForeground": "#C5C9D1",
    "titleBar.inactiveBackground": "#13101C",
    "titleBar.inactiveForeground": "#6A6482",
    "titleBar.border": "#1E1A2A",
    
    // ========== STATUS BAR ==========
    "statusBar.background": "#13101C",
    "statusBar.foreground": "#B8B4C8",
    "statusBar.border": "#1E1A2A",
    "statusBar.debuggingBackground": "#FF5F87",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.noFolderBackground": "#2D2640",
    "statusBarItem.activeBackground": "#2A243A",
    "statusBarItem.hoverBackground": "#1E1A2A",
    
    // ========== TERMINAL ==========
    "terminal.background": "#16131F",
    "terminal.foreground": "#C5C9D1",
    "terminal.ansiBlack": "#1A1625",
    "terminal.ansiRed": "#FF5F87",
    "terminal.ansiGreen": "#32CB32",
    "terminal.ansiYellow": "#FFD066",
    "terminal.ansiBlue": "#6EB8FF",
    "terminal.ansiMagenta": "#C173FF",
    "terminal.ansiCyan": "#32B9FF",
    "terminal.ansiWhite": "#C5C9D1",
    "terminal.ansiBrightBlack": "#6A6482",
    "terminal.ansiBrightRed": "#FF8AA8",
    "terminal.ansiBrightGreen": "#5AE65A",
    "terminal.ansiBrightYellow": "#FFE099",
    "terminal.ansiBrightBlue": "#99D4FF",
    "terminal.ansiBrightMagenta": "#D89FFF",
    "terminal.ansiBrightCyan": "#6DD4FF",
    "terminal.ansiBrightWhite": "#E0E4EB",
    
    // ========== PANEL ==========
    "panel.background": "#16131F",
    "panel.border": "#2A2438",
    "panelTitle.activeBorder": "#5C78FF",
    "panelTitle.activeForeground": "#E0E4EB",
    "panelTitle.inactiveForeground": "#7A7488",
    
    // ========== INPUT & DROPDOWN ==========
    "input.background": "#1C1826",
    "input.foreground": "#C5C9D1",
    "input.border": "#3A3050",
    "input.placeholderForeground": "#6A6482",
    "inputOption.activeBorder": "#5C78FF",
    "inputValidation.errorBackground": "#1C1826",
    "inputValidation.errorBorder": "#FF5F87",
    "inputValidation.warningBackground": "#1C1826",
    "inputValidation.warningBorder": "#FFB347",
    "dropdown.background": "#1C1826",
    "dropdown.foreground": "#C5C9D1",
    "dropdown.border": "#3A3050",
    
    // ========== BUTTONS ==========
    "button.background": "#5C78FF",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#7A8FFF",
    
    // ========== NOTIFICATIONS ==========
    "notificationCenter.border": "#3A3050",
    "notificationCenterHeader.background": "#1C1826",
    "notifications.background": "#1C1826",
    "notifications.border": "#3A3050",
    "notifications.foreground": "#C5C9D1",
    "notificationLink.foreground": "#6EB8FF",
    
    // ========== BADGE ==========
    "badge.background": "#5C78FF",
    "badge.foreground": "#FFFFFF",
    
    // ========== SCROLLBAR ==========
    "scrollbarSlider.background": "#3A305080",
    "scrollbarSlider.hoverBackground": "#4A4060A0",
    "scrollbarSlider.activeBackground": "#5A5070C0",
    
    // ========== PEEK VIEW ==========
    "peekView.border": "#5C78FF",
    "peekViewEditor.background": "#16131F",
    "peekViewEditor.matchHighlightBackground": "#5C78FF40",
    "peekViewResult.background": "#13101C",
    "peekViewResult.matchHighlightBackground": "#5C78FF40",
    "peekViewResult.selectionBackground": "#2D2640",
    "peekViewTitle.background": "#13101C",
    
    // ========== GIT DECORATIONS ==========
    "gitDecoration.modifiedResourceForeground": "#FFB347",
    "gitDecoration.deletedResourceForeground": "#FF5F87",
    "gitDecoration.untrackedResourceForeground": "#32CB32",
    "gitDecoration.ignoredResourceForeground": "#6A6482",
    "gitDecoration.conflictingResourceForeground": "#C173FF"
  },
  
  "tokenColors": [
    // ========== COMMENTI ==========
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A6482",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#7A7498",
        "fontStyle": "italic"
      }
    },
    
    // ========== KEYWORDS ==========
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#C173FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special Keywords (import, export, return)",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.return",
        "keyword.control.flow"
      ],
      "settings": {
        "foreground": "#BA5AFF",
        "fontStyle": "bold"
      }
    },
    
    // ========== JAVA/C# SPECIFIC ==========
    {
      "name": "Java Annotations",
      "scope": [
        "meta.annotation.java",
        "storage.type.annotation.java",
        "punctuation.definition.annotation.java"
      ],
      "settings": {
        "foreground": "#FFB347",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# Attributes",
      "scope": [
        "meta.attribute.csharp",
        "storage.type.attribute.csharp",
        "punctuation.definition.attribute.csharp"
      ],
      "settings": {
        "foreground": "#FFB347",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Java/C# Access Modifiers",
      "scope": [
        "storage.modifier.java",
        "storage.modifier.csharp",
        "storage.modifier.access"
      ],
      "settings": {
        "foreground": "#BA5AFF",
        "fontStyle": ""
      }
    },
    {
      "name": "C# var and dynamic",
      "scope": [
        "keyword.other.var.csharp",
        "keyword.type.dynamic.csharp"
      ],
      "settings": {
        "foreground": "#C173FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# LINQ Keywords",
      "scope": [
        "keyword.linq.csharp"
      ],
      "settings": {
        "foreground": "#D89FFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Generics Brackets",
      "scope": [
        "punctuation.definition.typeparameters",
        "meta.generic"
      ],
      "settings": {
        "foreground": "#7A8FFF"
      }
    },
    
    // ========== CONSTANTS ==========
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable.other.constant",
        "constant.language"
      ],
      "settings": {
        "foreground": "#A070FF"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.decimal",
        "constant.numeric.hex",
        "constant.numeric.octal",
        "constant.numeric.binary"
      ],
      "settings": {
        "foreground": "#FFD066",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#BA5AFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Null and Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.void"
      ],
      "settings": {
        "foreground": "#A070FF",
        "fontStyle": "italic"
      }
    },
    
    // ========== STRINGS ==========
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#32CB32"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#5AE65A"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "meta.template.expression",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#FFD066"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp",
        "punctuation.definition.string.regexp"
      ],
      "settings": {
        "foreground": "#32B9FF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#6DD4FF"
      }
    },
    
    // ========== VARIABLES ==========
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#C5C9D1"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#B38F00"
      }
    },
    {
      "name": "Properties and Members",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#FF2A9D"
      }
    },
    {
      "name": "This/Self",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#FF5F87",
        "fontStyle": "italic"
      }
    },
    
    // ========== FUNCTIONS ==========
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#6EB8FF"
      }
    },
    {
      "name": "Method Declaration",
      "scope": [
        "meta.method.declaration",
        "meta.method"
      ],
      "settings": {
        "foreground": "#6EB8FF"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": [
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#99D4FF"
      }
    },
    
    // ========== TYPES & CLASSES ==========
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#3299FF"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#5CB4FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Enums",
      "scope": [
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#7FC9FF"
      }
    },
    {
      "name": "Type Parameters (Generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#7A8FFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Primitive Types",
      "scope": [
        "storage.type.primitive",
        "support.type.primitive",
        "keyword.type"
      ],
      "settings": {
        "foreground": "#3299FF"
      }
    },
    {
      "name": "Namespace/Package",
      "scope": [
        "entity.name.namespace",
        "entity.name.type.namespace",
        "storage.type.namespace"
      ],
      "settings": {
        "foreground": "#A8C8FF"
      }
    },
    
    // ========== OPERATORS ==========
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#32B9FF"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#7A8FFF"
      }
    },
    
    // ========== DECORATORS ==========
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "entity.name.function.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#FFB347",
        "fontStyle": "italic"
      }
    },
    
    // ========== HTML ==========
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#C173FF"
      }
    },
    {
      "name": "HTML Tag Punctuation",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#A070FF"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFB347"
      }
    },
    
    // ========== JSX/TSX ==========
    {
      "name": "JSX Component",
      "scope": [
        "support.class.component",
        "entity.name.tag.tsx",
        "entity.name.tag.jsx"
      ],
      "settings": {
        "foreground": "#3299FF"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#FFB347"
      }
    },
    
    // ========== CSS ==========
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#C173FF"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#6EB8FF"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#32CB32"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#FFD066"
      }
    },
    
    // ========== JSON ==========
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.key.json"
      ],
      "settings": {
        "foreground": "#A070FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON String Values",
      "scope": [
        "string.value.json"
      ],
      "settings": {
        "foreground": "#32CB32"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "punctuation.separator.key-value.json",
        "punctuation.separator.dictionary.key-value.json"
      ],
      "settings": {
        "foreground": "#7A8FFF"
      }
    },
    
    // ========== MARKDOWN ==========
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#C173FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#FFD066",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#FF2A9D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#32CB32"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#6EB8FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#7A7498",
        "fontStyle": "italic"
      }
    },
    
    // ========== YAML ==========
    {
      "name": "YAML Keys",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#A070FF"
      }
    },
    
    // ========== SQL ==========
    {
      "name": "SQL Keywords",
      "scope": [
        "keyword.other.sql"
      ],
      "settings": {
        "foreground": "#C173FF",
        "fontStyle": "bold"
      }
    }
  ],
  
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // ========== VARIABLES ==========
    "variable": "#C5C9D1",
    "variable.readonly": "#A8C8FF",
    "variable.parameter": "#B38F00",
    "variable.property": "#FF2A9D",
    "variable.other.property": "#FF2A9D",
    
    // ========== FUNCTIONS & METHODS ==========
    "function": "#6EB8FF",
    "method": "#6EB8FF",
    "function.static": "#99D4FF",
    "method.static": "#99D4FF",
    
    // ========== TYPES ==========
    "class": "#3299FF",
    "interface": {
      "foreground": "#5CB4FF",
      "fontStyle": "italic"
    },
    "enum": "#7FC9FF",
    "enumMember": "#A070FF",
    "type": "#3299FF",
    "typeParameter": {
      "foreground": "#7A8FFF",
      "fontStyle": "italic"
    },
    "struct": "#5CB4FF",
    
    // ========== KEYWORDS ==========
    "keyword": "#C173FF",
    "this": {
      "foreground": "#FF5F87",
      "fontStyle": "italic"
    },
    
    // ========== CONSTANTS & LITERALS ==========
    "string": "#32CB32",
    "number": "#FFD066",
    "boolean": "#BA5AFF",
    
    // ========== PROPERTIES ==========
    "property": "#FF2A9D",
    "property.static": "#FF5F87",
    
    // ========== NAMESPACE ==========
    "namespace": "#A8C8FF",
    
    // ========== ANNOTATIONS/ATTRIBUTES ==========
    "annotation": {
      "foreground": "#FFB347",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#FFB347",
      "fontStyle": "italic"
    },
    
    // ========== MODIFIERS ==========
    "modifier": "#BA5AFF",
    
    // ========== COMMENTS ==========
    "comment": {
      "foreground": "#6A6482",
      "fontStyle": "italic"
    },
    
