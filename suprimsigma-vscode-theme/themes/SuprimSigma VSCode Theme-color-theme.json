{
	"$schema": "vscode://schemas/color-theme",
	"name": "SuprimSigma Enhanced Minimal",
	"type": "dark",
	"colors": {
		"focusBorder": "#00000000",
		"contrastBorder": "#00000000",
		"contrastActiveBorder": "#00000000",
		"editor.background": "#1A1625",
		"editor.foreground": "#C5C9D1",
		"editorCursor.foreground": "#A070FF",
		"editor.selectionBackground": "#2D2640",
		"editor.inactiveSelectionBackground": "#252034",
		"editor.selectionHighlightBackground": "#2A243A",
		"editor.lineHighlightBackground": "#1E1A2A",
		"editor.lineHighlightBorder": "#00000000",
		"editor.wordHighlightBackground": "#2D2640",
		"editor.wordHighlightStrongBackground": "#3A3050",
		"editor.findMatchBackground": "#5C78FF40",
		"editor.findMatchHighlightBackground": "#5C78FF20",
		"editor.hoverHighlightBackground": "#2A243A",
		"editor.rangeHighlightBackground": "#2A243A20",
		"editorLineNumber.foreground": "#6A6482",
		"editorLineNumber.activeForeground": "#A89FC8",
		"editorWhitespace.foreground": "#3A3048",
		"editorIndentGuide.background": "#2A2438",
		"editorIndentGuide.activeBackground": "#5C78FF",
		"editorRuler.foreground": "#2A2438",
		"editorBracketMatch.background": "#5C78FF30",
		"editorBracketMatch.border": "#5C78FF",
		"sideBar.background": "#16131F",
		"sideBar.foreground": "#C5C9D1",
		"sideBar.border": "#00000000",
		"sideBarTitle.foreground": "#B8B4C8",
		"sideBarSectionHeader.background": "#1A1625",
		"sideBarSectionHeader.foreground": "#B8B4C8",
		"sideBarSectionHeader.border": "#00000000",
		"activityBar.background": "#0F0D16",
		"activityBar.foreground": "#6EB8FF",
		"activityBar.inactiveForeground": "#4A5A70",
		"activityBar.border": "#00000000",
		"activityBar.activeBorder": "#6EB8FF",
		"activityBar.activeBackground": "#1A1625",
		"activityBar.activeFocusBorder": "#00000000",
		"activityBarBadge.background": "#6EB8FF",
		"activityBarBadge.foreground": "#000000",
		"list.activeSelectionBackground": "#2D2640",
		"list.activeSelectionForeground": "#E0E4EB",
		"list.inactiveSelectionBackground": "#252034",
		"list.inactiveSelectionForeground": "#C5C9D1",
		"list.hoverBackground": "#1E1A2A",
		"list.hoverForeground": "#E0E4EB",
		"list.focusBackground": "#2D2640",
		"list.focusOutline": "#00000000",
		"list.highlightForeground": "#6EB8FF",
		"tab.activeBackground": "#1A1625",
		"tab.inactiveBackground": "#13101C",
		"tab.activeForeground": "#E0E4EB",
		"tab.inactiveForeground": "#7A7488",
		"tab.activeBorder": "#5C78FF",
		"tab.activeBorderTop": "#00000000",
		"tab.border": "#00000000",
		"tab.hoverBackground": "#1E1A2A",
		"tab.hoverForeground": "#C5C9D1",
		"tab.unfocusedActiveForeground": "#B8B4C8",
		"tab.unfocusedInactiveForeground": "#6A6482",
		"titleBar.activeBackground": "#0F0D16",
		"titleBar.activeForeground": "#6EB8FF",
		"titleBar.inactiveBackground": "#0F0D16",
		"titleBar.inactiveForeground": "#4A5A70",
		"titleBar.border": "#00000000",
		"menubar.selectionForeground": "#6EB8FF",
		"menubar.selectionBackground": "#1A1625",
		"menubar.selectionBorder": "#00000000",
		"menu.foreground": "#C5C9D1",
		"menu.background": "#1C1826",
		"menu.selectionForeground": "#6EB8FF",
		"menu.selectionBackground": "#2D2640",
		"menu.selectionBorder": "#00000000",
		"menu.separatorBackground": "#00000000",
		"commandCenter.foreground": "#6EB8FF",
		"commandCenter.activeForeground": "#99D4FF",
		"commandCenter.background": "#0F0D16",
		"commandCenter.activeBackground": "#1A1625",
		"commandCenter.border": "#00000000",
		"commandCenter.inactiveForeground": "#4A5A70",
		"commandCenter.activeBorder": "#00000000",
		"button.background": "#5C78FF",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#5C78FF",
		"button.border": "#00000000",
		"button.focusOutline": "#00000000",
		"statusBar.background": "#13101C",
		"statusBar.foreground": "#B8B4C8",
		"statusBar.border": "#00000000",
		"statusBar.debuggingBackground": "#FF5F87",
		"statusBar.debuggingForeground": "#FFFFFF",
		"statusBar.noFolderBackground": "#2D2640",
		"statusBarItem.activeBackground": "#2A243A",
		"statusBarItem.hoverBackground": "#1E1A2A",
		"terminal.background": "#16131F",
		"terminal.foreground": "#E0E4EB",
		"terminalCursor.foreground": "#A070FF",
		"terminal.ansiBlack": "#1A1625",
		"terminal.ansiRed": "#FF5F87",
		"terminal.ansiGreen": "#32CB32",
		"terminal.ansiYellow": "#FFD066",
		"terminal.ansiBlue": "#6EB8FF",
		"terminal.ansiMagenta": "#C173FF",
		"terminal.ansiCyan": "#32B9FF",
		"terminal.ansiWhite": "#E0E4EB",
		"terminal.ansiBrightBlack": "#6A6482",
		"terminal.ansiBrightRed": "#FF8AA8",
		"terminal.ansiBrightGreen": "#5AE65A",
		"terminal.ansiBrightYellow": "#FFE099",
		"terminal.ansiBrightBlue": "#99D4FF",
		"terminal.ansiBrightMagenta": "#D89FFF",
		"terminal.ansiBrightCyan": "#6DD4FF",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"panel.background": "#16131F",
		"panel.border": "#00000000",
		"panelTitle.activeBorder": "#5C78FF",
		"panelTitle.activeForeground": "#E0E4EB",
		"panelTitle.inactiveForeground": "#7A7488",
		"input.background": "#1C1826",
		"input.foreground": "#C5C9D1",
		"input.border": "#00000000",
		"input.placeholderForeground": "#6A6482",
		"inputOption.activeBorder": "#00000000",
		"inputOption.activeBackground": "#2D2640",
		"inputOption.activeForeground": "#C5C9D1",
		"inputValidation.errorBackground": "#1C1826",
		"inputValidation.errorBorder": "#FF5F87",
		"inputValidation.warningBackground": "#1C1826",
		"inputValidation.warningBorder": "#FFB347",
		"dropdown.background": "#0F0D16",
		"dropdown.foreground": "#C5C9D1",
		"dropdown.border": "#00000000",
		"dropdown.listBackground": "#1C1826",
		"scrollbarSlider.background": "#3A305080",
		"scrollbarSlider.hoverBackground": "#4A4060A0",
		"scrollbarSlider.activeBackground": "#5A5070C0"
	},

	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": { "foreground": "#6A6482", "fontStyle": "italic" }
		},
		{
			"name": "Documentation Comments",
			"scope": ["comment.block.documentation", "comment.line.documentation"],
			"settings": { "foreground": "#7A7498", "fontStyle": "italic" }
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.other"],
			"settings": { "foreground": "#C173FF", "fontStyle": "" }
		},
		{
			"name": "Special Keywords",
			"scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from", "keyword.control.return", "keyword.control.flow"],
			"settings": { "foreground": "#BA5AFF", "fontStyle": "" }
		},
		{
			"name": "Java Annotations",
			"scope": ["meta.annotation.java", "storage.type.annotation.java", "punctuation.definition.annotation.java"],
			"settings": { "foreground": "#FFB347", "fontStyle": "italic" }
		},
		{
			"name": "C# Attributes",
			"scope": ["meta.attribute.csharp", "storage.type.attribute.csharp", "punctuation.definition.attribute.csharp"],
			"settings": { "foreground": "#FFB347", "fontStyle": "italic" }
		},
		{
			"name": "Java/C# Access Modifiers",
			"scope": ["storage.modifier.java", "storage.modifier.csharp", "storage.modifier.access"],
			"settings": { "foreground": "#BA5AFF", "fontStyle": "" }
		},
		{
			"name": "C# var and dynamic",
			"scope": ["keyword.other.var.csharp", "keyword.type.dynamic.csharp"],
			"settings": { "foreground": "#C173FF", "fontStyle": "italic" }
		},
		{
			"name": "C# LINQ Keywords",
			"scope": ["keyword.linq.csharp"],
			"settings": { "foreground": "#D89FFF", "fontStyle": "" }
		},
		{
			"name": "Generics Brackets",
			"scope": ["punctuation.definition.typeparameters", "meta.generic"],
			"settings": { "foreground": "#7A8FFF" }
		},
		{
			"name": "Constants",
			"scope": ["constant", "variable.other.constant", "constant.language"],
			"settings": { "foreground": "#A070FF" }
		},
		{
			"name": "Numbers",
			"scope": ["constant.numeric", "constant.numeric.decimal", "constant.numeric.hex", "constant.numeric.octal", "constant.numeric.binary"],
			"settings": { "foreground": "#FFD066", "fontStyle": "" }
		},
		{
			"name": "Boolean",
			"scope": ["constant.language.boolean"],
			"settings": { "foreground": "#BA5AFF", "fontStyle": "" }
		},
		{
			"name": "Null and Undefined",
			"scope": ["constant.language.null", "constant.language.undefined", "constant.language.void"],
			"settings": { "foreground": "#A070FF", "fontStyle": "italic" }
		},
		{
			"name": "Strings",
			"scope": ["string", "string.quoted", "punctuation.definition.string"],
			"settings": { "foreground": "#32CB32" }
		},
		{
			"name": "Template Strings",
			"scope": ["string.template", "string.interpolated"],
			"settings": { "foreground": "#5AE65A" }
		},
		{
			"name": "Template Expression",
			"scope": ["meta.template.expression", "punctuation.definition.template-expression"],
			"settings": { "foreground": "#FFD066" }
		},
		{
			"name": "Regex",
			"scope": ["string.regexp", "punctuation.definition.string.regexp"],
			"settings": { "foreground": "#32B9FF" }
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": { "foreground": "#6DD4FF" }
		},
		{
			"name": "Variables",
			"scope": ["variable", "variable.other"],
			"settings": { "foreground": "#C5C9D1" }
		},
		{
			"name": "Parameters",
			"scope": ["variable.parameter"],
			"settings": { "foreground": "#B38F00" }
		},
		{
			"name": "Properties and Members",
			"scope": ["variable.other.property", "variable.other.member", "entity.other.attribute-name", "support.type.property-name"],
			"settings": { "foreground": "#FF2A9D" }
		},
		{
			"name": "This/Self",
			"scope": ["variable.language.this", "variable.language.self", "variable.language.super"],
			"settings": { "foreground": "#FF5F87", "fontStyle": "italic" }
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "meta.function-call", "support.function"],
			"settings": { "foreground": "#6EB8FF" }
		},
		{
			"name": "Method Declaration",
			"scope": ["meta.method.declaration", "meta.method"],
			"settings": { "foreground": "#6EB8FF" }
		},
		{
			"name": "Built-in Functions",
			"scope": ["support.function.builtin"],
			"settings": { "foreground": "#99D4FF" }
		},
		{
			"name": "Classes",
			"scope": ["entity.name.type", "entity.name.class", "support.class"],
			"settings": { "foreground": "#3299FF" }
		},
		{
			"name": "Interfaces",
			"scope": ["entity.name.type.interface"],
			"settings": { "foreground": "#5CB4FF", "fontStyle": "italic" }
		},
		{
			"name": "Enums",
			"scope": ["entity.name.type.enum"],
			"settings": { "foreground": "#7FC9FF" }
		},
		{
			"name": "Type Parameters",
			"scope": ["entity.name.type.parameter"],
			"settings": { "foreground": "#7A8FFF", "fontStyle": "italic" }
		},
		{
			"name": "Primitive Types",
			"scope": ["storage.type.primitive", "support.type.primitive", "keyword.type"],
			"settings": { "foreground": "#3299FF" }
		},
		{
			"name": "Namespace/Package",
			"scope": ["entity.name.namespace", "entity.name.type.namespace", "storage.type.namespace"],
			"settings": { "foreground": "#A8C8FF" }
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator", "keyword.operator.arithmetic", "keyword.operator.assignment", "keyword.operator.logical", "keyword.operator.comparison", "keyword.operator.relational"],
			"settings": { "foreground": "#32B9FF" }
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation", "punctuation.separator", "punctuation.terminator"],
			"settings": { "foreground": "#7A8FFF" }
		},
		{
			"name": "Decorators",
			"scope": ["meta.decorator", "entity.name.function.decorator", "punctuation.decorator"],
			"settings": { "foreground": "#FFB347", "fontStyle": "italic" }
		},
		{
			"name": "HTML Tags",
			"scope": ["entity.name.tag", "meta.tag"],
			"settings": { "foreground": "#C173FF" }
		},
		{
			"name": "HTML Tag Punctuation",
			"scope": ["punctuation.definition.tag"],
			"settings": { "foreground": "#A070FF" }
		},
		{
			"name": "HTML Attributes",
			"scope": ["entity.other.attribute-name.html", "entity.other.attribute-name"],
			"settings": { "foreground": "#FFB347" }
		},
		{
			"name": "JSX Component",
			"scope": ["support.class.component", "entity.name.tag.tsx", "entity.name.tag.jsx"],
			"settings": { "foreground": "#3299FF" }
		},
		{
			"name": "JSX Attribute",
			"scope": ["entity.other.attribute-name.jsx", "entity.other.attribute-name.tsx"],
			"settings": { "foreground": "#FFB347" }
		},
		{
			"name": "CSS Selectors",
			"scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
			"settings": { "foreground": "#C173FF" }
		},
		{
			"name": "CSS Properties",
			"scope": ["support.type.property-name.css", "meta.property-name.css"],
			"settings": { "foreground": "#6EB8FF" }
		},
		{
			"name": "CSS Property Values",
			"scope": ["support.constant.property-value.css", "meta.property-value.css"],
			"settings": { "foreground": "#32CB32" }
		},
		{
			"name": "CSS Units",
			"scope": ["keyword.other.unit.css"],
			"settings": { "foreground": "#FFD066" }
		},
		{
			"name": "JSON Keys",
			"scope": ["support.type.property-name.json", "meta.structure.dictionary.key.json string.quoted.double.json", "meta.structure.dictionary.json string.quoted.double.json"],
			"settings": { "foreground": "#A070FF", "fontStyle": "" }
		},
		{
			"name": "JSON String Values",
			"scope": ["meta.structure.dictionary.value.json string.quoted.double.json"],
			"settings": { "foreground": "#32CB32" }
		},
		{
			"name": "JSON Punctuation",
			"scope": ["punctuation.separator.dictionary.pair.json", "punctuation.separator.dictionary.key-value.json", "punctuation.separator.array.json", "punctuation.definition.dictionary.begin.json", "punctuation.definition.dictionary.end.json"],
			"settings": { "foreground": "#7A8FFF" }
		},
		{
			"name": "JSON Brackets",
			"scope": ["punctuation.definition.array.begin.json", "punctuation.definition.array.end.json"],
			"settings": { "foreground": "#C173FF" }
		},
		{
			"name": "Markdown Headings",
			"scope": ["markup.heading", "entity.name.section.markdown"],
			"settings": { "foreground": "#C173FF", "fontStyle": "" }
		},
		{
			"name": "Markdown Bold",
			"scope": ["markup.bold"],
			"settings": { "foreground": "#FFD066", "fontStyle": "" }
		},
		{
			"name": "Markdown Italic",
			"scope": ["markup.italic"],
			"settings": { "foreground": "#FF2A9D", "fontStyle": "italic" }
		},
		{
			"name": "Markdown Code",
			"scope": ["markup.inline.raw", "markup.fenced_code"],
			"settings": { "foreground": "#32CB32" }
		},
		{
			"name": "Markdown Links",
			"scope": ["markup.underline.link"],
			"settings": { "foreground": "#6EB8FF", "fontStyle": "underline" }
		},
		{
			"name": "Markdown Quote",
			"scope": ["markup.quote"],
			"settings": { "foreground": "#7A7498", "fontStyle": "italic" }
		},
		{
			"name": "YAML Keys",
			"scope": ["entity.name.tag.yaml"],
			"settings": { "foreground": "#A070FF" }
		},
		{
			"name": "SQL Keywords",
			"scope": ["keyword.other.sql"],
			"settings": { "foreground": "#C173FF", "fontStyle": "" }
		}
	],

	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#C5C9D1",
		"variable.readonly": "#A8C8FF",
		"variable.parameter": "#B38F00",
		"variable.property": "#FF2A9D",
		"variable.other.property": "#FF2A9D",
		"function": "#6EB8FF",
		"method": "#6EB8FF",
		"function.static": "#99D4FF",
		"method.static": "#99D4FF",
		"class": "#3299FF",
		"interface": { "foreground": "#5CB4FF", "fontStyle": "italic" },
		"enum": "#7FC9FF",
		"enumMember": "#A070FF",
		"type": "#3299FF",
		"typeParameter": { "foreground": "#7A8FFF", "fontStyle": "italic" },
		"struct": "#5CB4FF",
		"keyword": "#C173FF",
		"this": { "foreground": "#FF5F87", "fontStyle": "italic" },
		"string": "#32CB32",
		"number": "#FFD066",
		"boolean": "#BA5AFF",
		"property": "#FF2A9D",
		"property.static": "#FF5F87",
		"namespace": "#A8C8FF",
		"annotation": { "foreground": "#FFB347", "fontStyle": "italic" },
		"decorator": { "foreground": "#FFB347", "fontStyle": "italic" },
		"modifier": "#BA5AFF",
		"comment": { "foreground": "#6A6482", "fontStyle": "italic" },
		"operator": "#32B9FF"
	}
}
